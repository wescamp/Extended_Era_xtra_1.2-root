#~ msgstr "bastó"

#~ msgid "impact"
#~ msgstr "impacte"

#~ msgid "chaos blast"
#~ msgstr "explosió de caos"

#~ msgid "cold"
#~ msgstr "fred"

#~ msgid "infernal storm"
#~ msgstr "tempesta infernal"

#~ msgid "fire"
